{
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/website-1",
      "destination": "/website 1/index.html"
    },
    {
      "source": "/website-2",
      "destination": "/website 2/index.html"
    },
    {
      "source": "/website-3",
      "destination": "/website 3/index.html"
    },
    {
      "source": "/website-1/:path*",
      "destination": "/website 1/:path*"
    },
    {
      "source": "/website-2/:path*",
      "destination": "/website 2/:path*"
    },
    {
      "source": "/website-3/:path*",
      "destination": "/website 3/:path*"
    }
  ]
}
